{
  "name": "tzatziki",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start-client-prod\" \"npm run start-server\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile-server": "tsc -p src/server/",
    "run-server": "node dist/out-tsc-server/server.js",
    "start-server": "nodemon --watch 'src/server/' -e ts --exec 'npm run compile-server && npm run run-server'",
    "start-server-ts": "nodemon --watch 'src/server/' --ignore 'src/server/**/*.spec.ts' --exec ts-node -- src/server/server.ts",
    "start-client-prod": "ng serve --proxy-config proxy.conf.json",
    "bundle": "npm run bundle-prod",
    "bundle-test": "npm run bundle-client-test && npm run bundle-server-test",
    "bundle-prod": "npm run bundle-client-prod && npm run bundle-server-prod",
    "bundle-client-test": "ng build",
    "bundle-client-prod": "ng build --prod --env=prod --aot && gulp compress",
    "bundle-server-test": "webpack --config webpack.server-config.js",
    "bundle-server-prod": "webpack --config webpack.server-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": ">=4.0.0-beta <5.0.0",
    "@angular/common": ">=4.0.0-beta <5.0.0",
    "@angular/compiler": ">=4.0.0-beta <5.0.0",
    "@angular/core": ">=4.0.0-beta <5.0.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": ">=4.0.0-beta <5.0.0",
    "@angular/http": ">=4.0.0-beta <5.0.0",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": ">=4.0.0-beta <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0-beta <5.0.0",
    "@angular/router": ">=4.0.0-beta <5.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.26",
    "bindings": "^1.2.1",
    "bluebird": "^3.5.0",
    "bootstrap": "^4.0.0-alpha.6",
    "chokidar": "^1.7.0",
    "cloudconvert": "^1.1.1",
    "core-js": "^2.4.1",
    "graceful-fs": "^4.1.11",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "hammerjs": "^2.0.8",
    "nodemailer": "^4.0.1",
    "oauth": "^0.9.15",
    "querystring": "^0.2.0",
    "rxjs": "^5.3.0",
    "sockjs": "^0.3.18",
    "thepiratebay": "1.4.2",
    "transmission": "^0.4.9",
    "xml2js": "^0.4.17",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.0",
    "@angular/compiler-cli": ">=4.0.0-beta <5.0.0",
    "@types/jasmine": "2.5.51",
    "@types/node": "~7.0.28",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-loader": "^2.0.3",
    "ts-node": "~3.0.6",
    "tslint": "~5.4.3",
    "typescript": "~2.3.4"
  }
}
